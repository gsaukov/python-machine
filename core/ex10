#list can be changed by calling and modifying
#tuple can not - fixwd
x = 5,6,7,8 # tupple
y = (5,6,7,3) # tupple

x = [5,6,7,8] # list
y = [5,6,7,3] # list

#tuple are great for sequence of unpacking,
#or when you have a list you never want changed

def function():
    return 12,6,8

q,r,s = function()
#when called this function will ALWAYS have the order of 12,6,8

print (q,r,s)
print (s,r,q)

def second():
    return [3,5,8]

b,i,g = second()

print(b,i,g)
print(g,b,i)

sample = ['today', 'tomorrow', 'yesterday', 'some other day']
print(sample)
print(len(sample))
print(sample.count('today'))

print(sample[2])

sample.insert(1, 'huge')
print(sample)


sample.insert(3, 'huger')
print(sample)

sample.remove('huge')
print(sample)

sample.reverse()
print(sample)

sample.pop(0)
print(sample)


sample.append('this will always be put in the last index position')
print(sample, '\n')
print('checkpt')

sample.sort()
print(sample)


print(sample[:2])
print(sample[2:4])


my_tupple = 'today', 'tomorrow', 'yesterday', 'some other day'
print(my_tupple)
print(len(my_tupple))



